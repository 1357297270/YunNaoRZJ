        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.StartPage_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.ClassRoomActivity" />
        <activity android:name=".view.ReadActivity" />
        <activity android:name=".view.ExtendActivity" />
        <activity android:name=".view.UpDataActivity" />
        <activity android:name=".view.Details_Activity" />
        <activity android:name=".view.ReadStart_Activity" />
        <activity android:name=".view.Test_xiangqing_Activity"></activity>
<activity android:name=".MainActivity"/>
    </application>

</manifest>